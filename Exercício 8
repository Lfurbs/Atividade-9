#include <stdio.h>

int main() {

    int vetor[6];

    printf("Informe 6 valores:\n");
    for (int i = 0; i < 6; i++) {
        scanf("%d", &vetor[i]);
    }

    printf("Valores lidos na ordem inversa:\n");
    for (int i = 5; i >= 0; i--) {
        printf("%d\n", vetor[i]);
    }

    return 0;
}
