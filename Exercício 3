#include <stdio.h>

int main() {

    float vetorOG[10];
    float vetorQua[10];

    printf("Digite 10 n√∫meros reais:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%f", &vetorOG[i]);
    }
    for (int i = 0; i < 10; i++) {
        vetorQua[i] = vetorOG[i] * vetorOG[i];
    }

    printf("Conjunto original:\n");
    for (int i = 0; i < 10; i++) {
        printf("%f ", vetorOG[i]);
    }
    printf("\n");

    printf("Quadrados dos componentes:\n");
    for (int i = 0; i < 10; i++) {
        printf("%f ", vetorQua[i]);
    }
    printf("\n");

    return 0;
}
