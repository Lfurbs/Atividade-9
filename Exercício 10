#include <stdio.h>

int main() {

    float notas[15];

    printf("Digite as notas de prova dos 15 alunos:\n");
    for (int i = 0; i < 15; i++) {
        scanf("%f", &notas[i]);
    }

    float soma = 0;
    for (int i = 0; i < 15; i++) {
        soma += notas[i];
    }
    float media = soma / 15;

    printf("A media geral das notas Ã©: %.2f\n", media);

    return 0;
}
