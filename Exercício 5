#include <stdio.h>

int main() {

    int vetor[10];

    printf("Informe 10 valores:\n");
    for (int i = 0; i < 10; i++) {
        scanf("%d", &vetor[i]);
    }

    int contadorPar = 0;
    for (int i = 0; i < 10; i++) {
        if (vetor[i] % 2 == 0) {
            contadorPar++;
        }
    }

    printf("O vetor possui %d valores pares.\n", contadorPar);

    return 0;
}
